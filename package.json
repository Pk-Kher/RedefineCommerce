  "scripts": {
    "start_local": "react-scripts start",
    "dev": "react-scripts start",
    "start": "npm run watch:css && npm run watch:css1 && react-scripts start",
    "start:development": "npm run watch:css&& npm run watch:css1 && env-cmd -f .env.development react-scripts start",
    "start:staging": "npm run watch:css&& npm run watch:css1 && env-cmd -f .env.staging react-scripts start",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/assets/css/tailwind/admin/index.css -o src/assets/css/tailwind/admin/output.css",
    "watch:css1": "postcss src/assets/css/tailwind/front/index.css -o src/assets/css/tailwind/front/output.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "10.4.5",
